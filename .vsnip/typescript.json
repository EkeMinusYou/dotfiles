{
  "import": {
    "prefix": "imp",
    "body": "import { $0 } from '$1';",
    "description": "import"
  },
  "import all": {
    "prefix": "ima",
    "body": "import * as ${0:M} from '$1';",
    "description": "import all"
  },
  "import jsr": {
    "prefix": "imj",
    "body": "import * as ${0:M} from 'jsr:$1';",
    "description": "import jsr"
  },
  "className with clsx, tailwind css, tempalte literals css": {
    "prefix": "cla",
    "body": "className={clsx('${1}', css`${2}`)}",
    "description": "className with clsx, tailwind css, tempalte literals css"
  },
  "className with clsx, tailwind css": {
    "prefix": "cl",
    "body": "className={clsx('${1}')}",
    "description": "className with clsx, tailwind css"
  }
}
